<h1> ログイン </h1>

<%= form_with(model: [@loginSession], url: login_path, local: true) do |f| %>
    <%= render 'layouts/error_messages', model: f.object %>
    <%= f.label :email, 'メールアドレス' %>
    <%= f.text_field :email %>
    <br>
    <%= f.label :password, 'パスワード' %>
    <%= f.password_field :password%>
    <br>
    <%= f.submit 'ログインする' %>
<% end %>
<br>
<%= link_to "ユーザー登録", new_user_path%>